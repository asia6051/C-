#include<bits/stdc++.h>
using namespace std;
int n;
queue <int>q;
stack<int>s;
int main()
{
    
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
cin >>n;
for(int i=0; i<n; i++){
	int a;
	cin >> a;
	s.push(a);
	q.push(a);
}
while(!s.empty()){
	cout << s.top()<<" ";
	s.pop();
}
cout << endl;
while(!q.empty()){
	cout << q.front()<< " ";
	q.pop();
}
   return 0; 
}
