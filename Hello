#include<bits/stdc++.h>
using namespace std;
string s;
int main(){
	string hej="hello";
	cin >> s;
	
	if(s.size() < 5){
		cout << "NO";
		return 0;
	}
	
	int i=0, k=0;
	while( i <= s.size()-1){
		if (hej[k] == s[i]) {
			i++;
			k++;
		}
		else i++;
		
		if(k == 5){
			cout << "YES";
			return 0;
		}
	}
	cout << "NO";
	return 0;
}
